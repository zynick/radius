{
  "name": "radius",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "npm run dev_boot && npm run dev_wapi & npm run dev_auth & npm run dev_acct",
    "dev_boot": "NODE_ENV=development DEBUG=radius   node bootstrap",
    "dev_acct": "NODE_ENV=development DEBUG=radius:* node acctServer",
    "dev_auth": "NODE_ENV=development DEBUG=radius:* node authServer",
    "dev_wapi": "NODE_ENV=development DEBUG=radius:* WEB_PORT=3001 node webServer",
    "start": "npm run start_boot && npm run start_wapi & npm run start_auth & npm run start_acct",
    "start_boot": "NODE_ENV=production DEBUG=radius       node bootstrap",
    "start_acct": "NODE_ENV=production DEBUG=radius:error node acctServer",
    "start_auth": "NODE_ENV=production DEBUG=radius:error node authServer",
    "start_wapi": "NODE_ENV=production DEBUG=radius:error WEB_PORT=3001 node webServer"
  },
  "dependencies": {
    "async": "^2.1.4",
    "body-parser": "^1.15.2",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "glob": "^7.1.1",
    "md5": "^2.2.1",
    "mongoose": "^4.7.5",
    "mongoose-unique-validator": "^1.0.3",
    "morgan": "^1.7.0",
    "radius": "^1.1.3"
  }
}
